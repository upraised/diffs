{
  "ecmaFeatures": {
    "modules": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "destructuring": true,
    "jsx": true,
    "templateStrings": true
  },
  "rules": {
    "brace-style": [2, "1tbs"],
    "camelcase": 2,
    "comma-style": [2, "last"],
    "consistent-this": [2, "that"],
    "curly": [2, "multi-line"],
    "dot-notation": 2,
    "eol-last": 0,
    "eqeqeq": [2, "smart"],
    "handle-callback-err": [2, "err"],
    "indent": [2, 2, {"indentSwitchCase": true}],
    "key-spacing": 2,
    "no-labels": 0,
    "new-cap": [2, {"capIsNew": false}],
    "no-floating-decimal": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-redeclare": 2,
    "no-shadow": 1,
    "no-space-before-semi": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "no-use-before-define": [2, "nofunc"],
    "no-warning-comments": [0, {"terms": ["todo"], "location": "anywhere"}],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "strict": 0
  },
  "env": {
    "node": true
  }
}
